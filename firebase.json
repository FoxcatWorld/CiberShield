{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/src/juegos.html",
        "destination": "/src/juegos.html"
      },
      {
        "source": "/src/proximamente.html",
        "destination": "/src/proximamente.html"
      },
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ]
  }
}
